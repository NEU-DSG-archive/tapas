<?php

/**
 * @file
 * TAPAS-specific interface tweaks to Drupal.
 */


/**
 * Implements hook_help().
 */

function randwikipage_help($path, $arg) {

	switch($path) {
		case "admin/help#randwikipage":
			return '<p>' . t("Interface tweaks made to Drupal for the TAPAS project.") . '</p>';
			break;
	}
}
