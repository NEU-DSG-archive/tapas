# Ignore configuration files that may contain sensitive information.
sites/*/*.settings*.php
sites/*/settings*.php
sites/*/settings*.php.*

# Ignore paths that contain user-generated content.
sites/*/files/projects/
sites/*/files/cache/
sites/*/files/style/
sites/*/files/pictures/
sites/*/files/tmp/
sites/*/files/bulk-temp/
sites/*/private/

# Ignore files that have passwords in them
.htpasswd

# Ignore .htaccess - we create this as a symlink to the appropriate .htaccess.env
# on a per machine basis.
.htaccess
